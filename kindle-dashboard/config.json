{
  "name": "Kindle Dashboard",
  "version": "1.0",
  "slug": "kindle_dashboard",
  "description": "A simple web interface to control Home Assistant entities, optimized for Kindle.",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {
    "8080/tcp": 8080
  },
  "webui": "http://[HOST]:[PORT:8080]",
  "ingress": true,
  "ingress_port": 8080,
  "panel_icon": "mdi:kindle",
  "panel_title": "Kindle Dashboard",
  "host_network": false,
  "options": {},
  "schema": {},
  "homeassistant_api": true,
  "hassio_api": true,
  "auth_api": true,
  "supervisor_api": true,
  "ingress_stream": true,
  "services_role": "default"
}